"Turn on line numbering "
"set nu"

" Higlhight search"
set hls

" use a vi-style line editing interface"
"set -o vi"

" Change colorscheme from default to delek"
"colorscheme desert
"colorscheme koehler
"colorscheme peachpuff
"colorscheme shine
"colorscheme zellner
colorscheme default
"colorscheme elflord
"colorscheme blue
"colorscheme ron
"colorscheme slate
"colorscheme pablo
"colorscheme murphy
"colorscheme morning
"colorscheme delek

" I like these one "
"colorscheme torte"
"colorscheme evening"
"colorscheme darkblue"


"set ai
set bg=dark
set showmatch
highlight SpecialKey ctermfg=DarkGray
"set listchars=tab:>-,trail:~
"set list
"autocmd BufRead *.py set smartindent cinwords=if,elif,else,for,while,try,except,finally,def,class"
"set tabstop=4
"set shiftwidth=4
"set expandtab
"set autoindent"
"set smartindent"
syntax on
"set listchars=tab:>-
"set listchars+=trail:.
"set ignorecase
"set smartcase
"map <C-t><up> :tabr<cr>
"map <C-t><down> :tabl<cr>
"map <C-t><left> :tabp<cr>
"map <C-t><right> :tabn<cr>
"set backspace=indent,eol,start


set visualbell
set t_vb=
